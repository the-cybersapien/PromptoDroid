<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AutoChangingTextView">
        <attr name="running" format="boolean" />
        <attr name="numberOfWords" format="integer" />
        <attr name="customText" format="string" />
        <attr name="speed" format="integer" />
        <attr name="promptType" format="enum">
            <enum name="line" value="0" />
            <enum name="words" value="1" />
        </attr>
    </declare-styleable>
</resources>